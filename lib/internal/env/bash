#! /bin/bash
#
# eval "$(%s env -)"

__go_func() {
  unset 'COMP_WORDS[0]'
  COMPREPLY=($("$_GO_SCRIPT" 'complete' "$((COMP_CWORD-1))" "${COMP_WORDS[@]}"))
  cd "$_GO_ROOTDIR"
}

_go_func() {
  local cmd="$1"
  shift

  case "$cmd" in
  unenv)
    complete -r '_go_func'
    unset -f '_go_func' '__go_func'
    ;;
  cd|pushd)
    "$cmd" "$_GO_ROOTDIR/$@"
    ;;
  *)
    _GO_CMD='_go_func' "$_GO_SCRIPT" "$cmd" "$@"
  esac
}

complete -o filenames -F __go_func _go_func
